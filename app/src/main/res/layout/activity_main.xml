<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout
        android:layout_width="1080px"
        android:layout_height="1920px"
        android:background="#fff"
        tools:ignore="MissingConstraints" />

        <LinearLayout
            android:layout_width="1080px"
            android:layout_height="1920px"
            android:orientation="vertical"
            tools:ignore="MissingConstraints">

            <LinearLayout
                android:layout_width="1080px"
                android:layout_height="384px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView56"
                    android:layout_width="10px"
                    android:layout_height="380px"
                    android:layout_weight="1"
                    android:background=" #03A9F4"
                    android:layout_margin="4dp"
                    android:gravity="center"/>

                <ImageView
                    android:id="@+id/imageView57"
                    android:layout_width="1px"
                    android:layout_height="380px"
                    android:layout_weight="1"
                    android:background=" #03A9F4"
                    android:layout_margin="4dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView58"
                    android:layout_width="352px"
                    android:layout_height="380px"
                    android:layout_weight="1"
                    android:background=" #03A9F4"
                    android:layout_margin="4dp" />

                <ImageView
                    android:id="@+id/imageView59"
                    android:layout_width="352px"
                    android:layout_height="380px"
                    android:layout_weight="1"
                    android:background=" #03A9F4"
                    android:layout_margin="4dp" />

                <ImageView
                    android:id="@+id/imageView60"
                    android:layout_width="352px"
                    android:layout_height="380px"
                    android:layout_weight="1"
                    android:background=" #03A9F4"
                    android:layout_margin="4dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="1080px"
                android:layout_height="760px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView61"
                    android:layout_width="532px"
                    android:layout_height="760px"
                    android:layout_margin="4dp"
                    android:layout_weight="1"
                    android:background=" #03A9F4" />

                <LinearLayout
                    android:layout_width="532px"
                    android:layout_height="760px"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView64"
                        android:layout_width="512px"
                        android:layout_height="760px"
                        android:layout_margin="4dp"
                        android:layout_weight="1"
                        android:background=" #03A9F4"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="380px"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView65"
                            android:layout_width="250px"
                            android:layout_height="380px"
                            android:layout_margin="4dp"
                            android:layout_weight="1"
                            android:background=" #03A9F4" />

                        <ImageView
                            android:id="@+id/imageView66"
                            android:layout_width="250px"
                            android:layout_height="380px"
                            android:layout_margin="4dp"
                            android:layout_weight="1"
                            android:background=" #03A9F4" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="1080px"
                android:layout_height="384px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView567"
                    android:layout_width="10px"
                    android:layout_height="135dp"
                    android:layout_margin="4dp"
                    android:layout_weight="1"
                    android:background=" #03A9F4"
                    android:gravity="center" />

                <ImageView
                    android:layout_width="1px"
                    android:layout_height="135dp"
                    android:layout_margin="4dp"
                    android:layout_weight="1"
                    android:background=" #03A9F4" />
            </LinearLayout>

        </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>